{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "insertPragma": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "proseWrap": "never",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "importOrder": [
    "^react",
    "^@components/(.*)$",
    "^@data/(.*)$",
    "^@hooks/(.*)$",
    "^@interfaces/(.*)$",
    "^@pages/(.*)$",
    "^@services/(.*)$",
    "^@store/(.*)$",
    "^@fortawesome/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
